	<script>
		$(function() {
			var _float = $("._float");
			var float1 = $(".float1");
			var float2 = $(".float2");
			var priceDetail = $("#priceDetail");
			var float_gold_phone = $(".float_gold_phone");
			var qr_code = $(".qr_code");
			var _timer = null;

			$(".float1").bind("mouseover", function(e) {
				clearTimeout(_timer);
				qr_code.hide();
				float2.removeClass("gold_after");
				float2.addClass("gold_before");
				priceDetail.show();
				if (!_float.hasClass("_float2")) {
